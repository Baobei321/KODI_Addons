msgctxt "Addon Summary"
msgid "Enable closed captioning and subtitles \"OnMute\"."
msgstr "Activa subtítulos cerrados y subtítulos \"OnMute\"."

msgctxt "Addon Description"
msgid "Let \"OnMute\" toggle closed captioning and subtitles while Kodi is muted."
msgstr "Permitir que \"OnMute\" cambie entre subtítulos cerrados y subtítulos mientras Kodi está silenciado."

msgctxt "#32001"
msgid "Enable Debugging"
msgstr "Activar depuración"

msgctxt "#32002"
msgid "Enable PVR Closed Captioning"
msgstr "Activar subtítulos cerrados de PVR"

msgctxt "#32003"
msgid "Search for Missing Subtitles"
msgstr "Buscar subtítulos faltantes"
